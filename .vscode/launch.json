{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Fastify (tsx)",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "tsx",
      "runtimeArgs": ["--inspect", "src/index.ts"],
      "skipFiles": ["<node_internals>/**"],
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env",
      "console": "integratedTerminal",
      "autoAttachChildProcesses": true
    }
  ]
}
